{
  "manifest_version": 3,
  "name": "Timer Chrome Extension",
  "version": "1.0",
  "description": "A Chrome extension built with React",
  "action": {},
  "permissions": [
    "activeTab",
    "background",
    "tabs",
    "scripting",
    "identity",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}

